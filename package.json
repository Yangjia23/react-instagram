{
  "name": "react-instagram",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./scripts/config/webpack.prod.js",
    "lint": "npm run lint-eslint && npm run lint-stylelint",
    "lint-eslint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint-stylelint": "stylelint --fix --config .stylelintrc.js src/**/*.{less,css,scss}",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "instagram",
    "react-instagram",
    "react-project",
    "typescript-project",
    "react-typescript",
    "react-ts-quick-starter"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js src"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
